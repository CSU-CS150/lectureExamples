public class conditionalsAndIfStatements {
	
	public static void ifstatements(int x, int y) {
		
		
		
			
		
		if(x > 100) {
			System.out.print("You entered the if");
		}
		else {
			System.out.println("You entered the else");
		}
		
	}

	public static void main(String[] args) {
		
		ifstatements(4, 2);
		

	}

}
